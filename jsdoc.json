{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [ "plugins/markdown", "plugins/summarize" ],
    "templates": {
        "outputSourceFiles": true,
        "systemName"            : "case-logger",
        "footer"                : "",
        "copyright"             : "Copyright Â© 2016 Case on it SL",
        "navType"               : "vertical",
        "theme"                 : "united",
        "linenums"              : true,
        "collapseSymbols"       : false,
        "inverseNav"            : true,
        "highlightTutorialCode" : true
    },
    "opts": {
      "template": "templates/docstrap/template",
      "encoding": "utf8",
      "private": true
    }
}